{
    "appConfig": {
        "appName": "MyCoolApp",
        "version": "1.0.0",
        "developer": {
            "name": "Jane Doe",
            "contact": "jane.doe@example.com",
            "website": "https://www.janedoe.com",
            "supportEmail": "support@mycoolapp.com",
            "socialMedia": {
                "twitter": "@MyCoolApp",
                "facebook": "https://facebook.com/MyCoolApp",
                "instagram": "https://instagram.com/MyCoolApp"
            }
        },
        "count": 3,
        "threshold": 3,
        "user_breach_alert": false,
        "settings": {
            "theme": "dark",
            "language": "en",
            "timezone": "UTC",
            "currency": "USD",
            "notifications": {
                "enabled": true,
                "sound": true,
                "vibrate": false,
                "emailAlerts": true,
                "pushNotifications": true,
                "newsletter": true
            },
            "privacy": {
                "tracking": false,
                "dataSharing": true,
                "thirdPartyAccess": false,
                "gdprCompliant": true,
                "adPersonalization": true
            },
            "security": {
                "twoFactorAuth": true,
                "passwordPolicy": {
                    "minLength": 8,
                    "requireSpecialChar": true,
                    "requireUppercase": true,
                    "requireNumber": true
                },
                "encryption": {
                    "enabled": true,
                    "type": "AES-256",
                    "keyManagement": "auto"
                }
            },
            "userProfile": {
                "allowProfilePicture": true,
                "allowLocationSharing": false,
                "maxProfileLength": 250
            }
        },
        "features": {
            "login": {
                "enabled": true,
                "socialLogin": {
                    "facebook": true,
                    "google": true,
                    "twitter": true
                },
                "emailVerification": true,
                "guestLogin": false
            },
            "socialMediaIntegration": {
                "enabled": true,
                "platforms": [
                    "Facebook",
                    "Twitter",
                    "Instagram"
                ]
            },
            "inAppPurchases": {
                "enabled": true,
                "purchaseItems": [
                    {
                        "id": "premium_version",
                        "name": "Premium Version",
                        "price": 9.99,
                        "currency": "USD"
                    },
                    {
                        "id": "extra_storage",
                        "name": "Extra Storage",
                        "price": 4.99,
                        "currency": "USD"
                    }
                ]
            },
            "analytics": {
                "enabled": true,
                "trackingId": "UA-12345678-1",
                "events": [
                    "app_launch",
                    "user_signup",
                    "purchase_made",
                    "error_occurred"
                ]
            },
            "chatSupport": {
                "enabled": true,
                "autoResponse": true,
                "responseTime": 5,
                "languages": [
                    "en",
                    "es",
                    "fr"
                ]
            }
        },
        "api": {
            "baseUrl": "https://api.mycoolapp.com",
            "version": "v1",
            "timeout": 5000,
            "retryAttempts": 3,
            "rateLimit": {
                "enabled": true,
                "maxRequestsPerMinute": 100,
                "resetInterval": 60
            },
            "auth": {
                "apiKey": "your-api-key-here",
                "oauth2": {
                    "enabled": true,
                    "clientId": "your-client-id",
                    "clientSecret": "your-client-secret"
                }
            },
            "endpoints": {
                "getUserData": "/user/data",
                "getUserNotifications": "/user/notifications",
                "updateProfile": "/user/update"
            }
        },
        "database": {
            "host": "db.mycoolapp.com",
            "port": 3306,
            "username": "app_user",
            "password": "secure_password",
            "databaseName": "app_db",
            "backupSchedule": "daily",
            "connectionPooling": true
        },
        "thirdPartyServices": {
            "paymentGateway": {
                "enabled": true,
                "provider": "Stripe",
                "apiKey": "your-stripe-api-key"
            },
            "emailService": {
                "enabled": true,
                "provider": "SendGrid",
                "apiKey": "your-sendgrid-api-key"
            },
            "cloudStorage": {
                "enabled": true,
                "provider": "AWS",
                "bucket": "mycoolapp-bucket",
                "region": "us-west-2"
            }
        },
        "localization": {
            "availableLanguages": [
                "en",
                "es",
                "fr",
                "de",
                "it",
                "pt"
            ],
            "defaultLanguage": "en",
            "translations": {
                "welcomeMessage": {
                    "en": "Welcome to MyCoolApp!",
                    "es": "\u00a1Bienvenido a MyCoolApp!",
                    "fr": "Bienvenue sur MyCoolApp!",
                    "de": "Willkommen bei MyCoolApp!"
                },
                "errorMessages": {
                    "en": "An error occurred. Please try again.",
                    "es": "Ocurri\u00f3 un error. Por favor, intente nuevamente.",
                    "fr": "Une erreur est survenue. Veuillez r\u00e9essayer.",
                    "de": "Es ist ein Fehler aufgetreten. Bitte versuchen Sie es erneut."
                }
            }
        },
        "logging": {
            "enabled": true,
            "level": "debug",
            "logToFile": true,
            "logToCloud": false,
            "maxLogSize": 5000
        },
        "updates": {
            "enabled": true,
            "checkInterval": 3600,
            "autoDownload": true,
            "autoInstall": true,
            "changelog": "https://www.mycoolapp.com/changelog"
        },
        "supportedDevices": [
            "android",
            "ios",
            "web"
        ],
        "deviceFeatures": {
            "android": {
                "minVersion": "5.0",
                "requiredPermissions": [
                    "ACCESS_FINE_LOCATION",
                    "INTERNET"
                ]
            },
            "ios": {
                "minVersion": "12.0",
                "requiredPermissions": [
                    "LOCATION_ALWAYS",
                    "NOTIFICATIONS"
                ]
            },
            "web": {
                "supportedBrowsers": [
                    "Chrome",
                    "Firefox",
                    "Safari",
                    "Edge"
                ]
            }
        }
    }
}